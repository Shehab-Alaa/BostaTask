<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:color="@color/ripple_effect"
    tools:targetApi="lollipop">
    <item
        android:id="@android:id/mask"
        tools:targetApi="lollipop">
        <shape android:shape="rectangle">
            <solid android:color="@color/ripple_effect" />
            <corners android:radius="@dimen/_6sdp" />
        </shape>
    </item>

    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/_6sdp"/>
            <solid android:color="@color/yellow_1"/>
            <padding android:top="@dimen/_6sdp" android:bottom="@dimen/_6sdp"
                android:right="@dimen/_6sdp" android:left="@dimen/_6sdp"/>
        </shape>
    </item>
</ripple>